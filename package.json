{
  "name": "cifront",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3030",
    "build": "tsc && vite build",
    "serve": "serve dist",
    "preview": "vite preview",
    "lint": "eslint --fix --ignore-path .eslintignore --ignore-pattern \"!**/.*\" .",
    "full-lint": "tsc && npm run lint",
    "prettier": "prettier --check src/**/*.{js,jsx,ts,tsx}",
    "init-husky": "husky install",
    "test": "jest",
    "cypress": "cypress open",
    "cypress_ci": "cypress run --headless"
  },
  "dependencies": {
    "husky": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.1.2",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.38.1",
    "@vitejs/plugin-react": "^2.1.0",
    "cypress": "^10.9.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "prettier": "^2.7.1",
    "serve": "^14.0.1",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2|svg)$": "<rootDir>/src/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
